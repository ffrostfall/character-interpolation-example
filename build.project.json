{
	"name": "character-interpolation",
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",

		"Workspace": {
			"$className": "Workspace",
			"$ignoreUnknownInstances": true,

			"map": { "$className": "Folder" },
			"characters": { "$className": "Folder" },
			"server_simulation": { "$className": "Camera" }
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",

			"server": { "$path": "build/server" }
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"shared": { "$path": "build/shared" },
			"packages": { "$path": "Packages" },
			"network": { "$path": "net/out/" },

			"client": { "$path": "build/client" }
		},

		"Players": {
			"$properties": {
				"CharacterAutoLoads": false
			}
		}
	}
}
